<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Controller">
   <children>
      <ComboBox fx:id="methodBox" layoutX="14.0" layoutY="14.0" prefWidth="150.0" />
      <TextField fx:id="urlField" layoutX="183.0" layoutY="14.0" prefHeight="26.0" prefWidth="204.0" promptText=" URL" />
      <ListView fx:id="filesBox" layoutX="14.0" layoutY="52.0" prefHeight="334.0" prefWidth="200.0" />
      <Button fx:id="sendButton" layoutX="402.0" layoutY="15.0" mnemonicParsing="false" onAction="#handleSend" text="Send" />
      <TextField fx:id="localFileField" layoutX="225.0" layoutY="360.0" promptText="Local file" />
      <Button fx:id="changeLocalFile" layoutX="387.0" layoutY="360.0" mnemonicParsing="false" onAction="#handleChangeLocalFile" text="Change" />
      <Label layoutX="236.0" layoutY="52.0" text="Status code: " />
      <Label fx:id="statusCodeLabel" layoutX="304.0" layoutY="52.0" />
      <Label layoutX="236.0" layoutY="85.0" text="Remote store path:" />
      <TextField fx:id="remoteStorePathField" layoutX="339.0" layoutY="81.0" />
   </children>
</AnchorPane>
